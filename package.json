    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "encoding": "^0.1.13",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "lightweight-charts": "^4.2.1",
    "lucide-react": "^0.456.0",
    "next": "^14.2.13",
